{
  "presets": [
    ["env", { "modules": false }],
    "es2015",
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "transform-object-assign",
    "transform-vue-jsx",
    "transform-object-rest-spread",
    // ["transform-runtime", {
    //   "polyfill": true,
    //   "regenerator": true
    // }],
  ],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}